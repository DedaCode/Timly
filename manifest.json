{
  "manifest_version": 3,
  "name": "Simple Pomodoro Timer",
  "version": "1.0",
  "description": "A simple Pomodoro timer to boost productivity",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "permissions": [
    "notifications",
    "storage"
  ],
  "icons": {
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
